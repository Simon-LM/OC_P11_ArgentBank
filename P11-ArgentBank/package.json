{
	"name": "p11-argentbank",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"vercel-build": "prisma generate && vite build",
		"build:strict": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest run",
		"test:dev": "vitest --mode development",
		"test:watch": "vitest watch",
		"typecheck": "tsc --noEmit",
		"prisma:test": "node -r ts-node/register src/prismaTest.ts",
		"lighthouse": "npx lighthouse@11.7.1 http://localhost:4173 --output=html --output-path=./lighthouse/reports/lighthouse-report.html --preset=perf --emulated-form-factor=mobile --only-categories=performance,accessibility,best-practices,seo --quiet",
		"lighthouse:mobile": "npx lighthouse@11.7.1 http://localhost:4173 --output=html --output-path=./lighthouse/reports/mobile-report.html --preset=perf --emulated-form-factor=mobile --only-categories=performance,accessibility,best-practices,seo --quiet",
		"lighthouse:desktop": "npx lighthouse@11.7.1 http://localhost:4173 --output=html --output-path=./lighthouse/reports/desktop-report.html --preset=perf --emulated-form-factor=desktop --only-categories=performance,accessibility,best-practices,seo --quiet",
		"lighthouse:json": "npx lighthouse@11.7.1 http://localhost:4173 --output=json --output-path=./lighthouse/reports/lighthouse-report.json --preset=perf --emulated-form-factor=mobile --only-categories=performance,accessibility,best-practices,seo --quiet",
		"lighthouse:profile": "npx lighthouse@11.7.1 http://localhost:4173/profile --output=html --output-path=./lighthouse/reports/profile-report.html --preset=perf --emulated-form-factor=mobile --only-categories=performance,accessibility,best-practices,seo --quiet",
		"lighthouse:dev": "npx lighthouse@11.7.1 http://localhost:3000 --output=html --output-path=./lighthouse/reports/dev-report.html --preset=perf --emulated-form-factor=mobile --only-categories=performance --quiet",
		"lighthouse:suite": "cd lighthouse && node scripts/lighthouse-test-suite.js",
		"lighthouse:analyze": "cd lighthouse && node lib/lighthouse-analyzer.js",
		"lighthouse:performance": "cd lighthouse && node lib/analyze-performance.js"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@datapunt/matomo-tracker-react": "^0.5.1",
		"@hookform/resolvers": "^3.9.1",
		"@prisma/client": "^6.6.0",
		"@radix-ui/react-label": "^2.1.1",
		"@reduxjs/toolkit": "^2.2.7",
		"@upstash/redis": "^1.34.8",
		"@vercel/kv": "^3.0.0",
		"bcrypt": "^5.1.1",
		"classnames": "^2.5.1",
		"dotenv": "^16.4.7",
		"jsonwebtoken": "^9.0.2",
		"normalize.css": "^8.0.1",
		"pg": "^8.14.1",
		"prisma": "^6.6.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.54.2",
		"react-icons": "^5.5.0",
		"react-intersection-observer": "^9.16.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.26.2",
		"redux-thunk": "^3.1.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/bcrypt": "^5.0.2",
		"@types/jest": "^29.5.13",
		"@types/node": "^22.14.1",
		"@types/react": "^18.3.20",
		"@types/react-dom": "^18.3.6",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-mock-store": "^1.0.6",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"@vitest/coverage-v8": "2.1.8",
		"@vitest/ui": "2.1.8",
		"chrome-launcher": "^1.2.0",
		"eslint": "^9.9.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"globals": "^15.9.0",
		"jsdom": "^25.0.1",
		"lighthouse": "^11.7.1",
		"lighthouse-cli": "^0.1.2",
		"msw": "^2.7.0",
		"prettier": "^3.4.2",
		"puppeteer": "^24.9.0",
		"redux-mock-store": "^1.5.4",
		"rollup-plugin-visualizer": "^5.14.0",
		"sass": "^1.79.4",
		"sass-embedded": "^1.78.0",
		"terser": "^5.39.2",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.0.1",
		"vercel": "^42.1.1",
		"vite": "^5.4.1",
		"vite-plugin-auto-alias": "^1.0.7",
		"vite-plugin-sass-dts": "^1.3.29",
		"vitest": "2.1.8"
	},
	"engines": {
		"node": "18.x",
		"npm": "9.x"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"prisma"
		],
		"onlyBuiltDependencies": [
			"bcrypt"
		]
	}
}
