{
  "name": "p11-argentbank",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && xdg-open dist/stats.html",
    "vercel-build": "prisma generate && vite build",
    "build:strict": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:dev": "vitest --mode development",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "prisma:test": "node -r ts-node/register src/prismaTest.ts",
    "lighthouse": "cd lighthouse && npm run test",
    "test:a11y": "node Pa11y/run-pa11y-tests.js",
    "test:a11y-custom": "node Pa11y/run-pa11y-tests.js",
    "test:a11y-update-port": "node Pa11y/update-port.js",
    "test:a11y-validate": "node Pa11y/validate-setup.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:a11y": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:a11y:report": "cypress run && npx mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && npx marge cypress/reports/merged-report.json --reportDir cypress/reports/html --inline",
    "test:e2e:report": "cypress run && npx mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && npx marge cypress/reports/merged-report.json --reportDir cypress/reports/html --inline",
    "cypress:clean": "./cypress/clean-reports.sh",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:all:a11y": "pnpm test:a11y && pnpm test:e2e:a11y",
    "clean": "bash scripts/clean-before-commit.sh",
    "clean:copilot": "bash scripts/clean-copilot-backups.sh",
    "sync": "bash scripts/sync-vscode.sh",
    "vercel:secrets": "bash scripts/get-vercel-secrets.sh",
    "sync:pnpm": "bash scripts/sync-pnpm.sh",
    "pre-commit": "bash scripts/clean-copilot-backups.sh && bash scripts/sync-pnpm.sh && bash scripts/vercel-config.sh prod && pnpm run lint:check && pnpm run format",
    "commit-ready": "pnpm vercel:config prod && git status",
    "clean:manual": "pnpm run clean:copilot && pnpm run sync:pnpm && echo '✅ Nettoyage manuel terminé'",
    "format": "prettier --write .",
    "lint:check": "eslint . --max-warnings 0",
    "vercel:dev": "bash scripts/vercel-config.sh dev && vercel dev",
    "vercel:prod": "bash scripts/vercel-config.sh prod && vercel --prod",
    "vercel:deploy": "bash scripts/vercel-deploy.sh",
    "vercel:deploy:preview": "bash scripts/vercel-deploy.sh preview",
    "vercel:deploy:production": "bash scripts/vercel-deploy.sh production",
    "vercel:config": "bash scripts/vercel-config.sh",
    "vercel:clean": "bash scripts/vercel-config.sh clean",
    "vercel:commit": "bash scripts/vercel-config.sh prod && echo '✅ vercel.json restauré pour le commit'"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-label": "^2.1.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@upstash/redis": "^1.34.8",
    "@vercel/kv": "^3.0.0",
    "bcrypt": "^5.1.1",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "normalize.css": "^8.0.1",
    "pg": "^8.14.1",
    "prisma": "^6.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "redux-thunk": "^3.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.9.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.13",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "axe-core": "^4.10.3",
    "chrome-launcher": "^1.2.0",
    "cypress": "^14.4.0",
    "cypress-axe": "^1.6.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "lighthouse": "^11.7.1",
    "lighthouse-cli": "^0.1.2",
    "markdownlint-cli": "^0.45.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "msw": "^2.7.0",
    "pa11y": "^9.0.0",
    "prettier": "^3.4.2",
    "puppeteer": "^24.9.0",
    "redux-mock-store": "^1.5.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.79.4",
    "sass-embedded": "^1.78.0",
    "terser": "^5.39.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.1",
    "vercel": "^42.1.1",
    "vite": "^5.4.1",
    "vite-plugin-auto-alias": "^1.0.7",
    "vite-plugin-sass-dts": "^1.3.29",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "@parcel/watcher",
      "@swc/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "bcrypt"
    ]
  }
}
